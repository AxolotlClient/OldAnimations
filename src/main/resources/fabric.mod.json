{
	"schemaVersion": 1,
	"id": "axolotlclient-oldanimations",
	"version": "${version}",
	"name": "Old Animations",
	"description": "Old Animations mod (extracted from Sol Client). Provides options for 1.7 visuals with AxolotlClient integration.",
	"authors": [
		"TheKodeToad",
		"moehreag"
	],
	"contact": {
		"homepage": "https://axolotlclient.github.io/",
		"sources": "https://github.com/AxolotlClient/OldAnimations/"
	},
	"license": "LGPL-3.0-or-later",
	"icon": "assets/axolotlclient/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"io.github.axolotlclient.oldanimations.OldAnimations::getInstance"
		],
		"axolotlclient.module": [
			"io.github.axolotlclient.oldanimations.OldAnimations::instance"
		],
		"modmenu": [
			"io.github.axolotlclient.oldanimations.OldAnimationsModmenu"
		]
	},
	"mixins": [
		"oldanimations.mixins.json"
	],
	"custom": {
		"modmenu:clientsideOnly": true
	},
	"depends": {
		"fabricloader": ">=0.14.7",
		"minecraft": "1.8.9"
	},
	"suggests": {
		"axolotlclient": "*"
	}
}
